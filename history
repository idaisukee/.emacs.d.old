;; -*- mode: emacs-lisp; coding: utf-8-unix -*-
;; Minibuffer history file, automatically generated by `savehist'.

(setq savehist-minibuffer-history-variables '(minibuffer-history file-name-history extended-command-history))
(setq minibuffer-history '("shell"))
(setq file-name-history '("~"))
(setq extended-command-history '("df" "dired"))
